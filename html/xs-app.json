{
  "welcomeFile": "webapp/index.html",
  "authenticationMethod": "route",
  "routes": [{
    "source": "/xsjs/(.*)(.xsjs)",
    "destination": "forecast_api",
    "csrfProtection": true,
    "authenticationType": "xsuaa",
    "scope": "$XSAPPNAME.USER"
  }, {
    "source": "/xsodata/(.*)(.xsodata)",
    "destination": "forecast_api",
    "csrfProtection": true,
    "authenticationType": "xsuaa",
    "scope": "$XSAPPNAME.USER"
  },{
    "source": "^/(.*)$",
    "localDir": "resources",
    "scope": "$XSAPPNAME.USER"
  }]
}